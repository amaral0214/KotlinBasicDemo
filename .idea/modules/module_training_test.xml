<?xml version="1.0" encoding="UTF-8"?>
<project name="module_training_test" default="compile.module.training_test">
  <dirname property="module.training_test.basedir" file="${ant.file.module_training_test}"/>
  
  <property name="module.jdk.home.training_test" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.training_test" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.training_test" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.training_test" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="training_test.output.dir" value="undefined"/>
  <property name="training_test.testoutput.dir" value="${module.training_test.basedir}/../../training/out/test/classes"/>
  
  <path id="training_test.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="training_test.module.production.classpath">
    <path refid="${module.jdk.classpath.training_test}"/>
    <pathelement location="${training_main.output.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_test.runtime.production.module.classpath">
    <path refid="training_main.runtime.production.module.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_test.module.classpath">
    <path refid="${module.jdk.classpath.training_test}"/>
    <pathelement location="${training_main.output.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_test.runtime.module.classpath">
    <pathelement location="${training_test.testoutput.dir}"/>
    <path refid="training_main.runtime.module.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.training_test">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.training_test">
    <patternset refid="excluded.from.module.training_test"/>
  </patternset>
  
  
  <target name="compile.module.training_test" depends="compile.module.training_test.production,compile.module.training_test.tests" description="Compile module training_test"/>
  
  <target name="compile.module.training_test.production" depends="register.custom.compilers,compile.module.training_main" description="Compile module training_test; production classes"/>
  
  <target name="compile.module.training_test.tests" depends="register.custom.compilers,compile.module.training_test.production" description="compile module training_test; test classes" unless="skip.tests"/>
  
  <target name="clean.module.training_test" description="cleanup module">
    <delete dir="${training_test.output.dir}"/>
    <delete dir="${training_test.testoutput.dir}"/>
  </target>
</project>