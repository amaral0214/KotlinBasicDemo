<?xml version="1.0" encoding="UTF-8"?>
<project name="module_kotlin_test" default="compile.module.kotlin_test">
  <dirname property="module.kotlin_test.basedir" file="${ant.file.module_kotlin_test}"/>
  
  <property name="module.jdk.home.kotlin_test" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.kotlin_test" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.kotlin_test" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.kotlin_test" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="kotlin_test.output.dir" value="undefined"/>
  <property name="kotlin_test.testoutput.dir" value="${module.kotlin_test.basedir}/../../out/test/classes"/>
  
  <path id="kotlin_test.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="kotlin_test.module.production.classpath">
    <path refid="${module.jdk.classpath.kotlin_test}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-allopen:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-noarg:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-model:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-api:1.2.71.classpath"/>
  </path>
  
  <path id="kotlin_test.runtime.production.module.classpath">
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-allopen:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-noarg:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-model:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-api:1.2.71.classpath"/>
  </path>
  
  <path id="kotlin_test.module.classpath">
    <path refid="${module.jdk.classpath.kotlin_test}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-allopen:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-noarg:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-model:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-api:1.2.71.classpath"/>
  </path>
  
  <path id="kotlin_test.runtime.module.classpath">
    <pathelement location="${kotlin_test.testoutput.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-allopen:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-noarg:1.2.71.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_junit:junit:4.12.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-model:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-gradle-plugin-api:1.2.71.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.kotlin_test">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.kotlin_test">
    <patternset refid="excluded.from.module.kotlin_test"/>
  </patternset>
  
  <path id="kotlin_test.module.test.sourcepath">
    <dirset dir="${module.kotlin_test.basedir}/../../src/test">
      <include name="java"/>
      <include name="kotlin"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.kotlin_test" depends="compile.module.kotlin_test.production,compile.module.kotlin_test.tests" description="Compile module kotlin_test"/>
  
  <target name="compile.module.kotlin_test.production" depends="register.custom.compilers" description="Compile module kotlin_test; production classes"/>
  
  <target name="compile.module.kotlin_test.tests" depends="register.custom.compilers,compile.module.kotlin_test.production" description="compile module kotlin_test; test classes" unless="skip.tests">
    <mkdir dir="${kotlin_test.testoutput.dir}"/>
    <javac2 destdir="${kotlin_test.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.kotlin_test}/javac">
      <compilerarg line="${compiler.args.kotlin_test}"/>
      <bootclasspath refid="kotlin_test.module.bootclasspath"/>
      <classpath refid="kotlin_test.module.classpath"/>
      <src refid="kotlin_test.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.kotlin_test"/>
    </javac2>
    
    <copy todir="${kotlin_test.testoutput.dir}">
      <fileset dir="${module.kotlin_test.basedir}/../../src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.kotlin_test.basedir}/../../src/test/kotlin">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.kotlin_test.basedir}/../../src/test/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.kotlin_test" description="cleanup module">
    <delete dir="${kotlin_test.output.dir}"/>
    <delete dir="${kotlin_test.testoutput.dir}"/>
  </target>
</project>