<?xml version="1.0" encoding="UTF-8"?>
<project name="module_training_main" default="compile.module.training_main">
  <dirname property="module.training_main.basedir" file="${ant.file.module_training_main}"/>
  
  <property name="module.jdk.home.training_main" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.training_main" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.training_main" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.training_main" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="training_main.output.dir" value="${module.training_main.basedir}/../../training/out/production/classes"/>
  <property name="training_main.testoutput.dir" value="${module.training_main.basedir}/../../training/out/production/classes"/>
  
  <path id="training_main.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="training_main.module.production.classpath">
    <path refid="${module.jdk.classpath.training_main}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_main.runtime.production.module.classpath">
    <pathelement location="${training_main.output.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_main.module.classpath">
    <path refid="${module.jdk.classpath.training_main}"/>
    <pathelement location="${training_main.output.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  <path id="training_main.runtime.module.classpath">
    <pathelement location="${training_main.output.dir}"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk8:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains.kotlin:kotlin-stdlib-common:1.2.71.classpath"/>
    <path refid="library.gradle:_org.jetbrains:annotations:13.0.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.training_main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.training_main">
    <patternset refid="excluded.from.module.training_main"/>
  </patternset>
  
  
  <target name="compile.module.training_main" depends="compile.module.training_main.production,compile.module.training_main.tests" description="Compile module training_main"/>
  
  <target name="compile.module.training_main.production" depends="register.custom.compilers" description="Compile module training_main; production classes"/>
  
  <target name="compile.module.training_main.tests" depends="register.custom.compilers,compile.module.training_main.production" description="compile module training_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.training_main" description="cleanup module">
    <delete dir="${training_main.output.dir}"/>
    <delete dir="${training_main.testoutput.dir}"/>
  </target>
</project>