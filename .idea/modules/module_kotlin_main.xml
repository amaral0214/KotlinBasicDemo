  <property name="compiler.args.kotlin_main" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="kotlin_main.output.dir" value="${module.kotlin_main.basedir}/../../out/production/classes"/>
