  <patternset id="excluded.from.module.kotlin_main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.kotlin_main">
    <patternset refid="excluded.from.module.kotlin_main"/>
  </patternset>
  
  <path id="kotlin_main.module.sourcepath">
    <dirset dir="${module.kotlin_main.basedir}/../../src/main">
      <include name="java"/>
      <include name="kotlin"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.kotlin_main" depends="compile.module.kotlin_main.production,compile.module.kotlin_main.tests" description="Compile module kotlin_main"/>
  
  <target name="compile.module.kotlin_main.production" depends="register.custom.compilers" description="Compile module kotlin_main; production classes">
    <mkdir dir="${kotlin_main.output.dir}"/>
    <javac2 destdir="${kotlin_main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.kotlin_main}/javac">
      <compilerarg line="${compiler.args.kotlin_main}"/>
      <bootclasspath refid="kotlin_main.module.bootclasspath"/>
      <classpath refid="kotlin_main.module.production.classpath"/>
      <src refid="kotlin_main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.kotlin_main"/>
    </javac2>
    
    <copy todir="${kotlin_main.output.dir}">
      <fileset dir="${module.kotlin_main.basedir}/../../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.kotlin_main.basedir}/../../src/main/kotlin">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.kotlin_main.basedir}/../../src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.kotlin_main.tests" depends="register.custom.compilers,compile.module.kotlin_main.production" description="compile module kotlin_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.kotlin_main" description="cleanup module">
    <delete dir="${kotlin_main.output.dir}"/>
    <delete dir="${kotlin_main.testoutput.dir}"/>
  </target>
</project>